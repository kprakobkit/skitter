  <a href="/user/<%= current_user.id %>/profile/show_users">all skeeters</a>
  <p>bio</p>
  <p>location</p>
</div>

<div>
  <% if @partial == "followees" %>
    <%= erb :followees %>
  <% elsif @partial == "followers" %>
    <%= erb :followers %>
  <% elsif @partial == "user_skeets" %>
    <%= erb :user_skeets %>
  <% elsif @partial == "show_users" %>
    <%= erb :show_users %>
  <% end %>
</div>


