<div class='followee-list' >
  <h1><%= @user.username %>'s followees</h1>
  <% @user.followees.each do |followee| %>
    <% unless followee.id == @user.id %>
      <div>
        <a href="/user/<%= followee.id %>/profile/user_skeets">
          <%= followee.username %>
        </a>
      </div>
    <% end %>
  <% end %>
</div>
